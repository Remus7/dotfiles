configuration{
    modi: "run,drun,window,combi";
    font: "FiraCode 12";
    icon-theme: "Oranchelo";
    show-icons: true;
    terminal: "alacritty";
    drun-display-format: "{icon} {name}";
    location: 0;
    disable-history: false;
    hide-scrollbar: true;
    display-combi: " 🖥️  All ";
    display-run: " 🏃  Run ";
    display-window: " 🪟  Window";
    display-drun: " 🐧  Drun";
    sidebar-mode: true;
}

@theme "kanagawa-wave"
